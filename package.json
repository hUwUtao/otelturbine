{
  "name": "otelturbine",
  "version": "0.1.0",
  "description": "OTLP/HTTP â†’ Prometheus remote-write pipeline library for Bun",
  "type": "module",
  "module": "index.ts",
  "types": "index.ts",
  "exports": {
    ".": {
      "bun": "./index.ts",
      "types": "./index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "index.ts",
    "README.md"
  ],
  "scripts": {
    "build": "bun build ./index.ts --outdir ./dist --target node --format esm --external snappy",
    "test": "bun test",
    "bench": "bun run bench.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun test && bun run build"
  },
  "keywords": [
    "otlp",
    "opentelemetry",
    "prometheus",
    "remote-write",
    "metrics",
    "bun",
    "elysia"
  ],
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "peerDependencies": {
    "typescript": "^5",
    "elysia": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "elysia": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "elysia": "^1.4.26",
    "mitata": "^1.0.34"
  },
  "dependencies": {
    "snappy": "^7.3.3"
  }
}
